{
    "name": "hytmng/php-generator",
    "description": "PHPのコードを定義・構築・ファイル出力までをサポートするライブラリ",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hytmng\\PhpGenerator\\": "src/PhpGenerator/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tominaga Hayato"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "php-coveralls/php-coveralls": "^2.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "upload-coverage": "./vendor/bin/php-coveralls --coverage_clover=coverage.xml -v"
    }
}
